<Window x:Class="Collection.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Collection"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="AddBTN" Content="Add" HorizontalAlignment="Center" Margin="0,330,0,0" VerticalAlignment="Top" Height="53" Width="528" FontFamily="Berlin Sans FB" Click="Add" FontSize="48"/>
        <TextBox x:Name="TEXTBOX" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Width="528" Height="36" FontSize="24" Margin="0,181,0,0" FontFamily="Berlin Sans FB"/>
        <TextBox x:Name="KeyTxt" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Width="528" Height="36" FontSize="24" Margin="0,181,0,0" FontFamily="Berlin Sans FB" Text="Key"/>
        <TextBox x:Name="ValuTxt" HorizontalAlignment="Center" Margin="0,254,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="528" Height="36" Text="Valu" FontFamily="Berlin Sans FB" FontSize="24"/>
    </Grid>
</Window>
